files:
  "/etc/httpd/conf.d/proxy.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      ProxyPass /websockets/ wss://www.petrasche.site/ upgrade=NONE
      ProxyPassReverse /websockets/ wss://www.petrasche.site/
      #Uncomment below for Option 3
      #ProxyPass / https://www.petrasche.site/
      #ProxyPassReverse / https://www.petrasche.site/
